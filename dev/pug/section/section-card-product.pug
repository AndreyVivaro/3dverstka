section.section-card-product.section-padding-bottom
	.container
		.row
			.col-xl-6.col-lg-6
				.wrap-sliders
					.swiper-container.gallery-thumbs.swiper-container-card-product-vertical
						.swiper-wrapper
							-for(let i=0; i<10; i++)
								.swiper-slide
									img(src="static/images/content/product/product2.png", alt="Anycubic i3 Mega X")
						//- .swiper-button-next.swiper-button-white
						//- .swiper-button-prev.swiper-button-white
					.swiper-container.gallery-top.swiper-container-card-product-big
						.swiper-wrapper
							-for(let i=0; i<10; i++)
								.swiper-slide
									a(href="static/images/content/product/product2.png" data-rel="lightcase")
										img.lightcase-card-product-js(src="static/images/content/product/product2.png", alt="Anycubic i3 Mega X")

			.col-xl-6.col-lg-6
				.product-description
					.product-description__info
						.rating
							span.rating__star &#9733;
							span.rating__star &#9733;
							span.rating__star &#9733;
							span.rating__star &#9733;
							span.rating__star &#9734;

						p.product-description__presence Есть в наличии
						// p.product-description__presence.product-description__presence_no Нет в наличии
						.product-description__info-icons
							div.product-description__info-icon.product-description__info-icon-basket-info-js
								span
								+icon("BasketInfo")
							div.product-description__info-icon
								span
								+icon("Top")
							div.product-description__info-icon
								span
								+icon("New")
							div.product-description__info-icon
								span 13%
								+icon("Sale")

					.product-description__wrap-name
						h3.product-description__name Anycubic i3 Mega X
					.product-description__quantity
						.product-description__wrap-price-text
							p.product-description__text-price.product-description__text-price-base.product-description__text-price_thround
								span(data-priceProduct="32900").number-space-js.price-product-js 32900
								span.rub &nbsp;
							p.product-description__text-price.product-description__text-price_sale
								span(data-priceProduct="59500").number-space-js.price-product-js 59500
								span.rub &nbsp;

						.wrap-recount.wrap-recount-js.quantity-block-js
							button.wrap-recount__recount-minus.recount-minus-js.quantity-arrow-minus-js -
							input(type="number", value="1").wrap-recount__recount-input.recount-input-js.inputQuantity-js
							button.wrap-recount__recount-plus.recount-plus-js.quantity-arrow-plus-js +

						.product-description__info-buttons
							button.product-description__info-button.product-description__info-button-js
								+icon("Scales")
							button.product-description__info-button.product-description__info-button-js
								+icon("Love")

					.product-description__buttons
						button.button.button-basket
							+icon("Basket")
							| В корзину
						button.button.button-basket
							+icon("message")
							| Купить в один клик
						button.button.button-basket
							+icon("card")
							| Купить в кредит

					.product-description__feature
						-for(let i=0; i<5; i++)
							p.product-description__feature-text
								span Производитель:
								span.product-description__feature-dotted
								span.product-description__feature-param 300x300x305 мм

		.row
			.col-xl-12
				nav.wrap-tabs-description
					#nav-tab.nav.nav-tabs(role='tablist')
						a#nav-tab-1.nav-item.nav-link.active(data-toggle='tab' href='#nav-content-1' role='tab' aria-controls='nav-content-1' aria-selected='true') Описание
						a#nav-tab-2.nav-item.nav-link(data-toggle='tab' href='#nav-content-2' role='tab' aria-controls='nav-content-2' aria-selected='false') Характеристики
